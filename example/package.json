{
  "name": "@wellcare/capacitor-plugin-starter-example",
  "version": "0.0.1",
  "description": "Capacitor Plugin Starter Example",
  "homepage": "https://github.com/wellcare/capacitor-plugin-starter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wellcare/capacitor-plugin-starter.git"
  },
  "license": "UNLICENSED",
  "author": "Wellcare",
  "scripts": {
    "analyze": "nuxt build --analyze",
    "build:plugin": "cd ../ && yarn build && cd example && npm run clean:plugin && node build-plugin.js",
    "build": "nuxt build --modern",
    "cap:android": "yarn generate && npx cap sync android && npx cap open android",
    "cap:ios": "yarn generate && npx cap sync ios && npx cap open ios",
    "cap:open:android": "npx cap open android",
    "cap:open:ios": "npx cap open ios",
    "dev": "npm run lint && nuxt",
    "export": "nuxt export",
    "generate-capacitor": "npm run lint && nuxt generate --no-ssr && npx cap copy",
    "generate": "npm run lint && nuxt generate",
    "lint": "eslint --ext .ts,.vue . src/ --fix",
    "serve": "nuxt serve",
    "start": "nuxt start",
    "clean:plugin" : "rimraf ./node_modules/@wellcare/capacitor-plugin-starter",
    "webpack": "nuxt webpack"
  },
  "dependencies": {
    "@mdi/js": "^7.0.0",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/composition-api": "^0.32.0",
    "@nuxtjs/dayjs": "^1.4.1",
    "@nuxtjs/i18n": "^7.2.0",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/toast": "^3.3.1",
    "@wellcare/capacitor-plugin-starter": "file:..",
    "@wellcare/vue-component": "^0.1.33",
    "axios": "^0.26.1",
    "cookie-universal-nuxt": "^2.1.5",
    "dotenv": "^16.0.1",
    "eslint-webpack-plugin": "^3.1.1",
    "nuxt-user-agent": "^1.2.2",
    "nuxt": "^2.15.8",
    "vuex-composition-map-fields": "^1.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@capacitor/android": "^4.0.0",
    "@capacitor/cli": "^4.0.0",
    "@capacitor/core": "^4.0.0",
    "@capacitor/ios": "^4.4.0",
    "@nuxt/types": "^2.15.3",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxt/typescript-runtime": "^2.1.0",
    "@nuxt/utils": "^2.15.8",
    "@nuxtjs/eslint-config-typescript": "^6.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/vuetify": "^1.12.3",
    "@types/node": "^14.14.33",
    "babel-core": "^7.0.0-bridge.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "7.20.0",
    "eslint-plugin-vuetify": "^1.1.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.0",
    "nuxt-typed-vuex": "^0.3.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "sass": "~1.32",
    "sass-loader": "^10.1.1",
    "stylelint": "^13.12.0",
    "stylelint-config-recommended": "^4.0.0",
    "ts-loader": "8.1.0",
    "typescript": "^4.6.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,vue}": [
      "eslint --ext .ts,.vue --fix"
    ]
  },
  "resolutions": {
    "vue": "2.6.14",
    "vue-server-renderer": "2.6.14",
    "vue-template-compiler": "2.6.14",
    "vuetify-loader": "1.7.3",
    "eslint-plugin-import": "2.26.0"
  }
}
